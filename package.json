{
  "name": "museo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npx vite --host",
    "build": "npx vite build",
    "preview": "npx vite preview",
    "format": "npx prettier -w ./",
    "deploy": "npx prettier -w ./ && npx vite build && firebase deploy --only hosting:museodelasparrandas",
    "lint": "npx eslint --ext .js,.jsx,.ts,.tsx,.vue src",
    "lint:fix": "npx eslint --ext .js,.jsx,.ts,.tsx,.vue src --fix",
    "push": "npm run format && git add . && git commit -m 'format' && git push"
  },
  "dependencies": {
    "aos": "^2.3.4",
    "bootstrap": "^5.2.3",
    "react-bootstrap": "^2.5.0",
    "react-icons": "^4.6.0",
    "firebase": "^9.14.0",
    "i": "^0.3.7",
    "react": "^18.2.0",
    "react-audio-player": "^0.17.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "6"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^2.2.0",
    "eslint": "^8.29.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-react": "^7.31.11",
    "vite": "^3.2.0"
  }
}
